{
  "name": "react-to-do-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.3.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.1",
    "react-scripts": "5.0.1",
    "redux": "4.0.1",
    "yarn": "^1.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "jsonServer": "json-server --watch src/mocks/tasks.json --port 3004"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@types/jest": "^24.0.15",
    "babel-jest": "24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "fetch-mock": "^7.3.3",
    "jest": "^24.7.1",
    "jest-fetch-mock": "^2.1.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/_mocks/styleMock.js"
    },
    "automock": false,
    "setupFiles": [
      "<rootDir>/_mocks/setupJest.js"
    ]
  }
}
